<div class="menu-container">
    <div id="menu">
        <input type="checkbox" id="toggle-nav" title="menu">
        <label for="toggle-nav" onclick><i class="fa fa-bars fa-lg small-circle"></i></label>
        <ul>
            {{#withSort pages "data.order" dir="asc"}}
                {{#is data.menu "true"}}
                    <li {{#is ../../page.dest this.dest}} class="active"{{/is}} >
                        <a  href ="{{relative ../../page.dest this.dest}}">
                            <div {{#isnt data.title "home"}} class="circle"{{/isnt}}>
                                {{#is data.title "home"}}
                                    <img src="https://raw.githubusercontent.com/audreyn/audreyn.github.io/master/assets/images/audrey.png" />
                                {{else}}
                                    {{data.title }}
                                {{/is}}
                            </div>
                        </a >
                    </li >
                {{/is}}
            {{/withSort}}
        </ul>
    </div>
</div>